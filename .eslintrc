{
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "graphql": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "strict": 0,
    "no-console": "off",
    "react/prop-types": [0]
  }
}
