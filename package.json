{
  "name": "amanda-brookfield",
  "description": "Website of the author Amanda Brookfield",
  "version": "2.0.0",
  "author": "Christian Allen",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@yarnpkg/pnpify": "^2.0.0-rc.21",
    "babel-plugin-styled-components": "^1.10.7",
    "date-fns": "^2.12.0",
    "gatsby": "^2.21.5",
    "gatsby-cli": "^2.12.7",
    "gatsby-image": "^2.4.0",
    "gatsby-link": "^2.4.0",
    "gatsby-plugin-google-analytics": "^2.3.0",
    "gatsby-plugin-layout": "^1.3.0",
    "gatsby-plugin-netlify": "^2.3.0",
    "gatsby-plugin-netlify-cms": "^4.3.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-styled-components": "^3.3.0",
    "gatsby-remark-images": "^3.3.0",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-source-instagram": "^0.7.0",
    "gatsby-transformer-remark": "^2.8.3",
    "gatsby-transformer-sharp": "^2.5.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.12.11",
    "netlify-lambda": "^1.6.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "styled-components": "^5.1.0",
    "styled-normalize": "^8.0.7",
    "titlecase": "^1.1.3",
    "uuid": "^8.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "yarn run develop",
    "clean": "yarn rimraf .cache public",
    "build": "yarn run clean && gatsby build",
    "build-functions": "netlify-lambda install ./src/lambda && netlify-lambda build ./src/lambda",
    "develop": "yarn run clean && gatsby develop",
    "serve": "gatsby serve",
    "serve-functions": "netlify-lambda serve functions-src",
    "lint": "eslint src",
    "format": "yarn run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "yarn run lint && yarn run prettier -- --list-different",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  }
}
